<div class="row">
  <div class="col-lg-9">
    <%= render 'item_list' %>
  </div>
  <div class="col-lg-3">
    <ul>
      <% @categories.each do |category| %>
        <li><%= category.name %>
          <ul>
            <% category.subcategories.each do |subcategory| %>
              <li><%= subcategory.name %></li>
            <% end %>
          </ul>
        </li>
      <% end %>
    </ul>
  </div>
</div>
